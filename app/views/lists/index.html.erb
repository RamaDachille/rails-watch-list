<h1>My Lists</h1>
<%= link_to "Create a new List", new_list_path %>
<br>
<p><%= @lists.count %> Lists</p>
<br>
<div id="lists">
  <% @lists.each do |list| %>
      <div class="card" style="width: 18rem;">
        <%# <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %>
        <img class="card-img-top" src="..." alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title"><%= link_to list.name, list %></h5>
        </div>
    </div>
  <% end %>
</div>
